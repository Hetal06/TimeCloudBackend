<div class="main-container" id="main-container">
	<div class="main-container-inner">
		<div class="page-content">
			<div class="row">
				<div class="col-sm-3">
					<div class="list-group">
						<a class="list-group-item active">
							List of Projects (Click Project to Edit)
						</a>
						<div data-ng-repeat="project in projectlist">
							<a popover-placement="top" popover-trigger="mouseenter" popover=" Click to Edit " popover-popup-delay="{{delayTime}}" href="#/editproject/{{project._id}}" class="list-group-item"><span class="pull-left">  {{project.projectname}} </span><i class="icon-edit pull-right"></i><div class="clearfix"></div></a>
						</div>
					</div>
				</div>
				<div class="col-sm-9">
					<div class="page-header">
						<h1>
							Time Cloud
							<small>
								<i class="icon-double-angle-right"></i>
								Create New Project
							</small>
						</h1>
					</div><!-- /.page-header -->
					<div class="row">
						<div class="col-xs-12">
							<div class="row">
								<div class="col-xs-12">
									<div class="space-4"></div>
									<div ng-show = "message.projectAdded != null">
										<div id="errors" class="span3 alert alert-success">{{message.projectAdded}}</div>
									</div>
									<div ng-show = "message.projectExist != null">
										<div id="errors" class="span3 alert alert-danger">{{message.projectExist}}</div>
									</div>									
									<div ng-show = "message.projectErr != null">
										<div id="errors" class="span3 alert alert-danger">{{message.projectErr}}
										</div>
									</div>
									<form class="form-horizontal" role="form" name="employeeFrm" method="post">
										<div class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="Enter Employee First Name - this is required" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1"> Name </label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.projectname" placeholder="Enter Project Name" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div class="space-4"></div>
										<div class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="Enter Employee Last Name - this is required" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1"> Client </label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.clientname" placeholder="Enter Client Name" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div  class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1"> JC/ID</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.JC" placeholder="JC/ID" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div  class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1"> Budget H</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.budgetH" placeholder="Budget Hours" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div  class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1">Budget $</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.budget$" placeholder="Budget $" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div  class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1">
												Supervisor H</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.supervisorH" placeholder="Supervisor Hours" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1"> Supervisor $</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.supervisor$" placeholder="Supervisor $" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="When set to YES project will show on home screen - if set to NO project will be found in Archived project " popover-popup-delay="{{delayTime}}" class="col-sm-3"> Active </label>
											<div class="col-sm-9">
												<input name="switch-field-1" data-ng-model="project.active" class="ace ace-switch ace-switch-5" type="checkbox" />
												<span class="lbl"></span>
											</div>
										</div>
										<div  class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1">Alerts</label>
											<div class="col-sm-9">
												<input type="text" id="form-field-1" data-ng-model="project.alerts" placeholder="Alerts" class="col-xs-10 col-sm-5" />
											</div>
										</div>
										<div class="form-group">
											<label popover-placement="top" popover-trigger="mouseenter" popover="" popover-popup-delay="{{delayTime}}" class="col-sm-3" for="form-field-1">Tasks</label>
											<div class="col-sm-5" id="taskParentdiv">
											</div>
											<div class="col-sm-1">
												<button id="updateEmp" popover-placement="top" popover-trigger="mouseenter" popover="Add new Task" popover-popup-delay="{{delayTime}}" type="button" class="btn btn-primary" data-ng-click="addtask()">
													<i class="icon-ok bigger-110"></i>
													Add Task
												</button>
											</div>
										</div>
										<div class="clearfix form-actions">
											<div class="col-md-offset-3 col-md-9">
												<button id="createEmp" popover-placement="top" popover-trigger="mouseenter" popover="Click to submit and save changes" popover-popup-delay="{{delayTime}}" type="button" class="btn btn-primary" data-ng-click="submitform()">
													<i class="icon-ok bigger-110"></i>
													Save
												</button>
											</div>
										</div>
									</form>
								</div>
							</div>
						</div><!-- /.col -->
					</div><!-- /.row -->
				</div><!--col-sm-9-->
			</div><!--row--fluid-->
		</div>
	</div><!-- /.main-container-inner -->
</div><!-- /.main-container -->


