<script type="text/ng-template" id="report-popup.html">		
	<div class="modal-header">
		<button type="button" class="close" ng-click="cancel()">&times;</button>
		<h4 class="modal-title">Change Report IP</h4>
	</div>
	<div class="modal-body">
		<div class="row form-group">
			<div class="col-xs-12">
				<div class="col-xs-12 span3 alert alert-success" ng-show="ipUpdated">
					Ip successfully saved
				</div>
				<div class="col-xs-12 span3 alert alert-success" ng-show="ipUpdatedFail">
					Failed to update IP
				</div>
			</div>

			<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
				<label>
					Report IP
				</label>
			</div>
			<div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
				<input type="text" placeholder="Enter Report ID" data-ng-model="user.reportsIp" class="form-control" id="cmpId">
			</div>
		</div>
	</div>
	<div class="modal-footer">
		<button type="button" class="btn btn-primary" ng-click="saveReportIp(user.reportsIp)">Save</button>
	</div>
</script>