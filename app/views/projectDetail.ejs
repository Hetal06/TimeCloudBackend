<div class="main-container-inner">
	<div class="page-content">
		<div class="row">
			<div class="page-header">
				<h1>
					Project
					<small>
						<i class="icon-double-angle-right"></i>
						Home
					</small>
				</h1>
			</div><!-- /.page-header -->
			<div class="col-xs-12">
				<div>
					<form class="" role="search" id="searchfrm" ng-submit="getSearch()" >
			      <div>
			        <input type="text" class="form-control" placeholder="Search" ng-model="searchtext" id="searchText">
			      </div>
		        <input type="submit" id="submit" value="Submit" style="visibility:hidden"/>
			    </form>
				</div>				
				<div class="table-responsive">
					<table id="sample-table-2" class="table table-striped table-bordered table-hover">
						<thead>
							<tr>	
								<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Employee No" popover-popup-delay="{{delayTime}}"> Project Name</label></th>
								<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by First Name" popover-popup-delay="{{delayTime}}"> Client Name </label> </th>
								<th>  <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Last Name" popover-popup-delay="{{delayTime}}">  Tasks </label>  </th>
								<!-- <th>
								<label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Shift Pattern" popover-popup-delay="{{delayTime}}">  % </label> </th>

								<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Department" popover-popup-delay="{{delayTime}}" style="width:20%"> Chart </label>
								</th>

								<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Status" popover-popup-delay="{{delayTime}}"> Status </label> </th>	 -->
							</tr>
						</thead>

						<tbody data-ng-repeat="project in project| filter:searchtext">						 
							<tr>
								<td>
									{{project.projectname}}
								</td>
								<td>
									{{project.clientname}}
								</td>
								<td style="padding: 0px;" ng-show="project.tasks">
									<table id="sample-table-2" class="table table-striped table-bordered table-hover" style="margin: 0px;">
										<thead>
											<tr>	
												<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Employee No" popover-popup-delay="{{delayTime}}">Task Name</label></th>
												<th> <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by First Name" popover-popup-delay="{{delayTime}}"> Task Code </label> </th>
												<th>  <label id="sort" popover-placement="top" popover-trigger="mouseenter" popover="Sort by Last Name" popover-popup-delay="{{delayTime}}">  Total </label>  </th>
											</tr>
										</thead>
										<tbody data-ng-repeat="task in project.tasks">						 
											<tr>
												<td>
													{{task.taskname}}
												</td>
												<td>

													{{task.taskCode}}
												</td>
												<td>
													{{task.total}}
												</td>
											</tr>
										</tbody>
									</table>

									<!-- <div class="row" >
										<div class="col-lg-4" >
											
										</div>
										<div class="col-lg-4" >
											
										</div>
										<div class="col-lg-4" >
											
										</div>
										
									</div>
									 -->
								</td>
								<td style="padding:0px;" ng-hide="project.tasks">
									---
								</td>
								<!-- 
								<td>
									{{project.usedH * 100 / project.budgetH | filterPecentage}} / {{project.used$ * 100 / project.budget$ | filterPecentage}}
									
								</td>
								<td my-attr="dollardiv_{{$index}}"  id="dollardiv_{{$index}}" reset-dollarchart style="text-align: center;width:15%;">
									
								</td>
								<td >
									<span ng-if="project.active">Active</span>
									<span ng-if="!project.active">In-Active</span>
									
								</td> -->
							</tr>
						</tbody> 
					</table>
				</div>
			</div>			
		</div><!--row--fluid-->
	</div><!-- /.main-container-inner -->
</div><!-- /.main-container -->
