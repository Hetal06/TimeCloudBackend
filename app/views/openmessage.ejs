<div class="main-container-inner">
	<div class="page-content">
		<div class="row">			
			<div class="col-sm-2">
		
			</div>
			<div class="col-sm-10">
	
		
			<div class="page-header">
				<h1>
					Time Cloud
					<small>
						<i class="icon-double-angle-right"></i>
						 <span popover-placement="top" popover-trigger="mouseenter" popover="tooltip2" popover-popup-delay="{{delayTime}}"> Messages  </span>
					</small>
				</h1>
			</div><!-- /.page-header -->
				<div class="row">
					<div class="col-sm-12">
						</br>
						<div class="row">
							
							<div class="col-sm-10">
							<div class="row">
								<span class="pull-right">
									<a popover-placement="top" popover-trigger="mouseenter" popover="Back to Messages" popover-popup-delay="{{delayTime}}" class="btn btn-primary " href="#/usermessages"><i class="icon-reply"></i>Back</a> 
								</span>
							</div>
								<div class="form-group">
									<label> Subject : </label> 
									<span>
									 {{messageData.subject}}
									</span>
									<span class="inline pull-right">
										<label> Received : </label> 
										<span>
										 {{messageData.received | utcTime}}
										</span>
									</span>
								</div>
								</br>
								<div class="form-group">
									<label> Message : </label> 
									<span>
									 {{messageData.message}}
									</span>
									
								</div>
							</div>
							
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>	